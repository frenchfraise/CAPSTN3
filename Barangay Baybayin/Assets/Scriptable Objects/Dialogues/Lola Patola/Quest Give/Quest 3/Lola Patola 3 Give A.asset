  dialogues:
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'Oh Sakoro, you''ve returned.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'We''ve been able to farm again, with your help.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'The problem is what''s next.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'We store our crops on the crop shack, right of the field.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'But the recent storm did a number on it.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'We can fix it up, if you can gather the following.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'We need [15 Superb Nipa] and [15 Superb Bamboo].

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'Superb Nipa is found in the deepest parts of the Forest, while Superb
      Bamboo is at the Bamboo Thicket, where the water is.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
  - character: {fileID: 11400000, guid: d07ffa9457a5d704b91096369ccebe08, type: 2}
    emotion: 10
    words: 'We''re counting on you, sweetie.

'
    backgroundSprite: {fileID: 0}
    speechTransitionType: 1
